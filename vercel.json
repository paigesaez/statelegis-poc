{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-fund --no-audit",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--max_old_space_size=4096",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
